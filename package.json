{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:https": "next dev --turbopack --experimental-https --port 8443",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "db:generate": "drizzle-kit generate --name",
    "db:migrate:dev": "wrangler d1 migrations apply my-db --local",
    "db:drop-sandbox": "wrangler d1 execute my-db --local --command \"UPDATE organization SET inquiryId = NULL;\"",
    "db:migrate:remote": "wrangler d1 migrations apply my-db --remote",
    "d1:cache:clean": "wrangler d1 execute my-db --command \"DELETE FROM tags; DELETE FROM revalidations\" --remote",
    "d1:cache:seed": "wrangler d1 execute my-db --file .open-next/cloudflare/cache-assets-manifest.sql --remote",
    "email:dev": "email dev -d src/react-email -p 3001",
    "postinstall": "patch-package"
  },
